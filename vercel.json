{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "enhanced-lesson-planner.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "enhanced-lesson-planner.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
